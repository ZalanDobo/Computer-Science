first_row=list(' '*3)
second_row=list(' '*3)
third_row=list(' '*3)
print(first_row)
print(second_row)
print(third_row)
xrow=list('x'*3)
yrow=list('y'*3)
while True:
    xcordinate=input('Enter the co-ordinate(Player X)(example:first row second column=12:')
    print(xcordinate)
    xcordinate=list(xcordinate)
    if xcordinate[0]=='1':
        seccord=xcordinate[1]
        realseccord=int(seccord)-1
        first_row[realseccord]='x'
        print('this is the realseccord:',realseccord)
        print(first_row)
        print(second_row)
        print(third_row)
        cross1x=list(first_row[0]+second_row[1]+third_row[2])
        cross2x=list(first_row[0]+second_row[1]+third_row[2])
        vertical1x=list(first_row[0]+second_row[0]+third_row[0])
        vertical2x=list(first_row[1]+second_row[1]+third_row[1])
        vertical3x=list(first_row[2]+second_row[2]+third_row[2])
        if first_row==xrow:
            print('Player X won')
            break
        elif cross1x==xrow:
            print('Player X won')
            break
        elif cross2x==xrow:
            print('Player X won')
            break
        elif vertical1x==xrow:
            print('Player X won')
        elif vertical2x==xrow:
            print('Player X won')
        elif vertical3x==xrow:
            print('Player X won')
    elif xcordinate[0]=='2':
        seccord=xcordinate[1]
        realseccord=int(seccord)-1
        second_row[realseccord]='x'
        print('this is the realseccord:',realseccord)
        print(first_row)
        print(second_row)
        print(third_row)
        cross1x=list(first_row[0]+second_row[1]+third_row[2])
        cross2x=list(first_row[2]+second_row[1]+third_row[0])
        vertical1x=list(first_row[0]+second_row[0]+third_row[0])
        vertical2x=list(first_row[1]+second_row[1]+third_row[1])
        vertical3x=list(first_row[2]+second_row[2]+third_row[2])
        if second_row==xrow:
            print('Player X won')
            break
        elif cross1x==xrow:
            print('Player X won')
            break
        elif cross2x==xrow:
            print('Player X won')
            break
        elif vertical1x==xrow:
            print('Player X won')
            break
        elif vertical2x==xrow:
            print('Player X won')
            break
        elif vertical3x==xrow:
            print('Player X won')
            break
    elif xcordinate[0]=='3':
        seccord=xcordinate[1]
        realseccord=int(seccord)-1
        third_row[realseccord]='x'
        print('this is the realseccord:',realseccord)
        print(first_row)
        print(second_row)
        print(third_row)
        cross1x=list(first_row[0]+second_row[1]+third_row[2])
        cross2x=list(first_row[2]+second_row[1]+third_row[0])
        vertical1x=list(first_row[0]+second_row[0]+third_row[0])
        vertical2x=list(first_row[1]+second_row[1]+third_row[1])
        vertical3x=list(first_row[2]+second_row[2]+third_row[2])
        if third_row==xrow:
            print('Player Y won')
            break
        elif cross1x==xrow:
            print('Player X won')
            break
        elif cross2x==xrow:
            print('Player X won')
            break
        elif vertical1x==xrow:
            print('Player X won')
            break
        elif vertical2x==xrow:
            print('Player X won')
            break
        elif vertical3x==xrow:
            print('Player X won')
            break
    ycordinate=input('Enter the co-ordinate(Player Y)(example:first row second column=12:')
    print(ycordinate)
    if ycordinate[0]=='1':
        seccord=ycordinate[1]
        realseccord=int(seccord)-1
        first_row[realseccord]='y'
        print('this is the realseccord:',realseccord)
        print(first_row)
        print(second_row)
        print(third_row)
