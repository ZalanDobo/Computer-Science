        cross2y=list(first_row[2]+second_row[1]+third_row[0])
        vertical1y=list(first_row[0]+second_row[0]+third_row[0])
        vertical2y=list(first_row[1]+second_row[1]+third_row[1])
        vertical3y=list(first_row[2]+second_row[2]+third_row[2])
        if first_row==yrow:
            print('Player Y won')
            break
        elif cross1y==yrow:
            print('Player Y won')
            break
        elif cross2y==yrow:
            print('Player Y won')
            break
        elif vertical1y==yrow:
            print('Player X won')
            break
        elif vertical2y==yrow:
            print('Player X won')
            break
        elif vertical3y==yrow:
            print('Player X won')
            break
    elif ycordinate[0]=='2':
        seccord=ycordinate[1]
        realseccord=int(seccord)-1
        second_row[realseccord]='y'
        print('this is the realseccord:',realseccord)
        print(first_row)
        print(second_row)
        print(third_row)
        cross1y=list(first_row[0]+second_row[1]+third_row[2])
        cross2y=list(first_row[2]+second_row[1]+third_row[0])
        vertical1y=list(first_row[0]+second_row[0]+third_row[0])
        vertical2y=list(first_row[1]+second_row[1]+third_row[1])
        vertical3y=list(first_row[2]+second_row[2]+third_row[2])
        if second_row==yrow:
            print('Player Y won')
            break
        elif cross1y==yrow:
            print('Player Y won')
            break
        elif cross2y==yrow:
            print('Player Y won')
            break
        elif vertical1y==yrow:
            print('Player X won')
            break
        elif vertical2y==yrow:
            print('Player X won')
            break
        elif vertical3y==yrow:
            print('Player X won')
            break
    elif ycordinate[0]=='3':
        seccord=ycordinate[1]
        realseccord=int(seccord)-1
        third_row[realseccord]='y'
        print('this is the realseccord:',realseccord)
        print(first_row)
        print(second_row)
        print(third_row)
        cross1y=list(first_row[0]+second_row[1]+third_row[2])
        cross2y=list(first_row[2]+second_row[1]+third_row[0])
        vertical1y=list(first_row[0]+second_row[0]+third_row[0])
        vertical2y=list(first_row[1]+second_row[1]+third_row[1])
        vertical3y=list(first_row[2]+second_row[2]+third_row[2])
